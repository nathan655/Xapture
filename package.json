{
  "name": "be.thomasmore.xapture",
  "displayName": "Xapture",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the device ready event.",
  "main": "index.js",
  "scripts": {
    "browser": "phonegap serve",
    "android": "cordova run android",
    "icon": "cordova-res android --type icon"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-badge": "^0.8.8",
    "cordova-plugin-camera": "github:apache/cordova-plugin-camera",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-local-notification": "^0.9.0-beta.2",
    "cordova-plugin-network-information": "^3.0.0",
    "cordova-plugin-printer": "^0.8.0",
    "cordova-plugin-save-image": "^0.3.0",
    "cordova-plugin-sqlite-2": "^1.0.7"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {},
      "cordova-plugin-sqlite-2": {},
      "cordova-plugin-printer": {
        "ANDROID_SUPPORT_V4_VERSION": "28.+"
      },
      "cordova-plugin-network-information": {},
      "cordova-plugin-local-notification": {},
      "cordova-plugin-save-image": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  },
  "dependencies": {
    "jquery": "^3.6.0"
  }
}